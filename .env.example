# Docker Deployment Environment Variables

# JWT Secret - CHANGE THIS IN PRODUCTION!
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRES_IN=7d

# CORS Origin (set to your frontend domain in production)
CORS_ORIGIN=*

# Default admin password (only used on first run)
DEFAULT_ADMIN_PASSWORD=admin123

# Original React DevTools settings (only for browser extension development)
# REACT_DEVTOOLS_AUTO_START=true
# REACT_DEVTOOLS_FORCE_FETCH=false
# REACT_DEVTOOLS_PORT=8097
# REACT_DEVTOOLS_CACHE_DURATION=86400000

